production:
  database_config_path: '/var/vcap/jobs/cf-mysql-broker/config/database.yml'
  log_path: '/var/vcap/sys/log/cf-mysql-broker/cf-mysql-broker.log'
  auth_token: '<%= properties.broker.auth_token %>'

  services:
  <% (properties.services || []).each do |service| %>
  - id: <%= service.id %>
    name: <%= service.name %>
    description: <%= service.description %>
    bindable: <%= service.bindable ? true : false %>
    plans:
    <% (service.plans || []).each do |plan| %>
    - id: <%= plan.id %>
      name: <%= plan.name %>
      description: <%= plan.description %>
      max_storage_mb: <%= plan.max_storage_mb %>
      max_user_connections: <%= plan.max_user_connections %>
    <% end %>
  <% end %>
